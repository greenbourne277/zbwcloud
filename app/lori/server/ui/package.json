{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --setupTestFrameworkScriptFile=./tests/setup.js",
    "lint": "vue-cli-service lint",
    "codegen": "rm -rf src/generated-sources/openapi; openapi-generator-cli generate -i ../../api/src/main/openapi/openapi.yaml -o src/generated-sources/openapi -g typescript-fetch --additional-properties=supportsES6=true,npmVersion=7.20.3,typescriptThreePlus=true",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\""
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.0-alpha.44",
    "@vuelidate/validators": "^2.0.0-alpha.31",
    "core-js": "^3.24.1",
    "neverthrow": "^4.4.2",
    "pinia": "^2.0.17",
    "ts-enum-util": "^4.0.2",
    "vue": "^2.7.8",
    "vue-i18n": "^8.27.2",
    "vue-i18n-bridge": "^9.2.0",
    "vue-router": "^3.5.4",
    "vuetify": "^2.6.8"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^3.3.0",
    "@pinia/testing": "^0.0.13",
    "@types/jest": "^27.5.2",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-router": "^4.5.19",
    "@vue/cli-plugin-typescript": "^4.5.19",
    "@vue/cli-plugin-unit-jest": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^1.3.0",
    "eslint": "^8.21.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.7.1",
    "sass": "^1.49.11",
    "sass-loader": "^10.3.1",
    "typescript": "^4.7.4",
    "vue-cli-plugin-i18n": "~2.3.1",
    "vue-cli-plugin-vuetify": "^2.5.1",
    "vue-template-compiler": "^2.7.8",
    "vuetify-loader": "^1.9.2"
  }
