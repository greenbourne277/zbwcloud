{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit --setupTestFrameworkScriptFile=./tests/setup.js",
    "lint": "vue-cli-service lint",
    "codegen": "rm -rf src/generated-sources/openapi; openapi-generator-cli generate -i ../../api/src/main/openapi/openapi.yaml -o src/generated-sources/openapi -g typescript-fetch --additional-properties=supportsES6=true,npmVersion=7.20.3,typescriptThreePlus=true",
    "i18n:report": "vue-cli-service i18n:report --src \"./src/**/*.?(js|vue)\" --locales \"./src/locales/**/*.json\""
  },
  "dependencies": {
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.3",
    "core-js": "^3.32.0",
    "lodash.isequal": "^4.5.0",
    "material-design-icons-iconfont": "^6.7.0",
    "neverthrow": "^4.4.2",
    "pinia": "^2.1.6",
    "ts-enum-util": "^4.0.2",
    "vue": "^2.7.14",
    "vue-i18n": "^8.28.2",
    "vue-i18n-bridge": "^9.2.2",
    "vue-router": "^3.6.5",
    "vuetify": "^2.7.1"
  },
  "devDependencies": {
    "@intlify/vue-i18n-loader": "^3.3.0",
    "@pinia/testing": "^0.1.3",
    "@types/jest": "^27.5.2",
    "@types/lodash.isequal": "^4.5.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vue/cli-plugin-babel": "^4.5.19",
    "@vue/cli-plugin-router": "^4.5.19",
    "@vue/cli-plugin-typescript": "^4.5.19",
    "@vue/cli-plugin-unit-jest": "^4.5.19",
    "@vue/cli-service": "^4.5.19",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "@vue/test-utils": "^1.3.6",
    "eslint": "^8.46.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.8.8",
    "sass": "^1.64.2",
    "sass-loader": "^10.4.1",
    "typescript": "^4.9.5",
    "vue-cli-plugin-i18n": "~2.3.2",
    "vue-cli-plugin-vuetify": "^2.5.8",
